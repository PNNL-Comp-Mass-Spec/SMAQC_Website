{
    "name": "pnnl-comp-mass-spec/smaqc_website",
    "type": "project",
    "description": "PNNL Omics SMAQC Site",
    "homepage": "https://prismsupport.pnl.gov/smaqc/",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    }
}
